//throw

public class Throw{
	public static void validate (int age){
		if(age<18){
			throw new ArithmeticException("is not eligible to vote");
		}
		else{
			System.out.println("is eligible to vote");
		}
	}
	public static void main(String [] args){
		validate(18);
		System.out.println("rest the code....");
	}
}

//throws

//throws

class Excep{
	void validate (int age) throws IOException{
		if(age<18)
			throw new IOException("not valid");
		else
			System.out.println("valid to vote");
	}
}
class ExceptionHandling{
	public static void main(String [] args) throws IOException{
		Excep obj = new Excep();
		obj.validate(18);
		System.out.println("rest of the code....");
	}
} 

//finally

class Finally1{
	public static void main(String [] args){
		try{
			int data = 25/5;
			System.out.println(data);
		}
		catch(NullPointerException e){
			System.out.println(e);
		}
		finally{
			System.out.println("finally block is executed....");
		}
	System.out.println("rest of the code....");
	}
}
			
